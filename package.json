  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Clinical-Support-Systems/bootstrap-iconpicker.git"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^7.1.0",
    "chai": "^4.1.0",
    "grunt": "^0.4.0",
    "grunt-contrib-clean": "^0.1.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-cssmin": "^0.12.3",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "mocha": "^3.4.2"
  },
  "scripts": {
    "browser": "index.html",
    "verify:fa7": "node util/verify-fa7.js",
    "verify:fa6": "node util/verify-fa6.js",
    "verify:fa5": "node util/verify-fa5.js",
    "verify:fa7-pro": "node util/verify-fa7-pro.js",
    "verify:fa6-pro": "node util/verify-fa6-pro.js",
    "verify:fa5-pro": "node util/verify-fa5-pro.js",
    "verify-all": "node util/verify-all.js",
    "strip:fa5": "node util/strip-fa5-svg.js",
    "strip:fa6": "node util/strip-fa6-svg.js",
    "strip:fa7": "node util/strip-fa7-svg.js",
    "strip:all": "npm run strip:fa5 && npm run strip:fa6 && npm run strip:fa7",
    "verify:iconsets": "node util/generate-fa-iconset.js --metadata icons-6.7.2.json --out src/js/iconset/iconset-fontawesome-6-all.js --version 6.7.2 --iconset-key fontawesome_6 --check && node util/generate-fa-iconset.js --metadata icons-7.1.0.json --out src/js/iconset/iconset-fontawesome-7-all.js --version 7.1.0 --iconset-key fontawesome_7 --check"
  }
}
